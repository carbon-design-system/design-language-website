{
  "extends": ["wesbos"],
  "plugins": ["react-hooks"],
  "rules": {
    "react/prop-types": 0,
    "import/no-extraneous-dependencies": 0,
    "react/destructuring-assignment": 0,
    "arrow-body-style": 0,
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}
